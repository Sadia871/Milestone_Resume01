{
    "version": 2,
    "builds": [
      {
        "src": "script.ts",
        "use": "@vercel/node"
      },
      {
        "src": "milestone.html",
        "use": "@vercel/static"
      },
      {
        "src": "style.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  ,
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ]
  } 